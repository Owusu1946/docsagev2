{
    "version": 2,
    "builds": [
        {
            "src": "src/serverless.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/serverless.ts"
        }
    ],
    "env": {
        "API_PORT": "3000"
    },
    "functions": {
        "src/serverless.ts": {
            "maxDuration": 60
        }
    }
}